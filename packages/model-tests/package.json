{
  "name": "@ai-sdk/model-tests",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test:capabilities": "vitest run src/test-runner.test.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/cerebras": "0.1.6",
    "@ai-sdk/deepinfra": "0.1.6",
    "@ai-sdk/deepseek": "0.1.6",
    "@ai-sdk/fireworks": "0.1.6",
    "@ai-sdk/google": "1.1.6",
    "@ai-sdk/google-vertex": "2.1.6",
    "@ai-sdk/luma": "0.0.3",
    "@ai-sdk/perplexity": "0.0.4",
    "@ai-sdk/openai": "1.1.5",
    "@ai-sdk/provider": "1.0.6",
    "@ai-sdk/provider-utils": "2.1.5",
    "@ai-sdk/togetherai": "0.1.6",
    "@ai-sdk/xai": "1.1.6",
    "ai": "4.1.11",
    "dotenv": "16.4.5",
    "vitest": "2.1.4"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@vercel/ai-tsconfig": "workspace:*",
    "tsup": "^8",
    "typescript": "5.6.3",
    "zod": "3.23.8"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://sdk.vercel.ai/docs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/ai.git"
  },
  "bugs": {
    "url": "https://github.com/vercel/ai/issues"
  },
  "keywords": [
    "ai"
  ]
}
